{
    "manifest_version": 2,
    "name": "Mikah's Utility Extension",

    "version": "0.1.2",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["js/jquery-3.2.1.min.js", "content.js", "js/index.js"]
    }],
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "webRequest",
        "*://*.google.com/"
    ]

}
